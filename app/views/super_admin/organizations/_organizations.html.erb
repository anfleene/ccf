<table class="table table-striped table-hover table-condensed table-bordered">
  <thead>
    <th>Organization</th>
    <th>Subdomain</th>

    <th>Actions</th>
  </thead>
  <tbody>
    <%@organizations.each do |o|%>
    <tr>
      <td><%=link_to o.name, admin_root_url(subdomain: o.subdomain)%></td>
      <td><%=o.subdomain%></td>
      <td>
        <div class="btn-group">
          <%=link_to "Edit", edit_super_admin_organization_path(o), :class => "btn btn-sm btn-default"%>
        </div>
      </td>
    </tr>
    <%end%>
  </tbody>
</table>
<%=will_paginate @organizations%>